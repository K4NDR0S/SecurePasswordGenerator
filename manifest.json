{
  "manifest_version": 2,
  "name": "Secure Password Generator",
  "version": "1.0",
  "description": "Generates secure passwords with user-defined length.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
